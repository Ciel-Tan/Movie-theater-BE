{
  "openapi": "3.0.0",
  "info": {
    "title": "Movie Theater API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:3000",
      "description": "Development server"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "description": "JWT Bearer token to access protected endpoints"
      }
    }
  },
  "paths": {
    "/api/movies/get": {
      "get": {
        "tags": [
          "Movies Controller"
        ],
        "responses": {
          "200": {
            "description": "List of movies",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "movie_id": {
                        "type": "integer"
                      },
                      "title": {
                        "type": "string"
                      },
                      "description": {
                        "type": "string"
                      },
                      "age_rating": {
                        "type": "integer"
                      },
                      "run_time": {
                        "type": "integer"
                      },
                      "release_date": {
                        "type": "string"
                      },
                      "trailer_link": {
                        "type": "string"
                      },
                      "language": {
                        "type": "string"
                      },
                      "background_poster_image": {
                        "type": "string"
                      },
                      "small_poster_image": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/movies/{movie_id}": {
      "get": {
        "tags": [
          "Movies Controller"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "movie_id",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Movie details"
          }
        }
      }
    },
    "/api/auth/login": {
      "post": {
        "tags": ["Authentication Controller"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "string",
                    "format": "email",
                    "description": "User's email address"
                  },
                  "password": {
                    "type": "string",
                    "description": "User's password"
                  }
                },
                "required": [
                  "email",
                  "password"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Login successful, JWT token generated",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "token": {
                      "type": "string"
                    },
                    "expiresIn": {
                      "type": "string",
                      "format": "date-time"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/auth/register": {
      "post": {
        "tags": ["Authentication Controller"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "full_name": {
                    "type": "string",
                    "description": "User's full name",
                    "example": "John Doe"
                  },
                  "gender": {
                    "type": "boolean",
                    "enum": [
                      true,
                      false
                    ],
                    "description": "User's gender",
                    "example": true
                  },
                  "birthday": {
                    "type": "string",
                    "format": "date",
                    "description": "User's birthday (YYYY-MM-DD)",
                    "example": "1990-01-15"
                  },
                  "id_number": {
                    "type": "string",
                    "description": "User's ID number",
                    "example": "ID123456789"
                  },
                  "phone_number": {
                    "type": "string",
                    "description": "User's phone number",
                    "example": "123-456-7890"
                  },
                  "email": {
                    "type": "string",
                    "format": "email",
                    "description": "User's email address (must be unique)",
                    "example": "john.doe@example.com"
                  },
                  "password": {
                    "type": "string",
                    "description": "User's password",
                    "example": "P@$$wOrd123"
                  }
                },
                "required": [
                  "full_name",
                  "gender",
                  "birthday",
                  "id_number",
                  "phone_number",
                  "email",
                  "password"
                ]
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Account created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Account created successfully"
                    },
                    "account": {
                      "type": "object",
                      "description": "Details of the newly created account"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/accounts/getAll": {
      "get": {
        "tags": ["Account Controller"],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "account_id": { "type": "integer" },
                      "full_name": { "type": "string" },
                      "email": { "type": "string", "format": "email" },
                      "gender": { "type": "boolean", "enum": [true, false] },
                      "birthday": { "type": "string", "format": "date" },
                      "id_number": { "type": "string" },
                      "phone_number": { "type": "string" },
                      "role": {
                        "type": "object",
                        "properties": {
                          "role_id": { "type": "integer" },
                          "role_name": { "type": "string" }
                        }
                      },
                      "membership_type": {
                        "type": "object",
                        "properties": {
                          "membership_id": { "type": "integer" },
                          "membership_name": { "type": "string" },
                          "discount_rate": { "type": "number", "format": "float" }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/accounts/{account_id}": {
      "get": {
        "tags": ["Account Controller"],
        "parameters": [
          {
            "in": "path",
            "name": "account_id",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "type": "object",
                      "description": "Account details when a specific account ID is provided (number).",
                      "properties": {
                        "account_id": { "type": "integer" },
                        "full_name": { "type": "string" },
                        "email": { "type": "string", "format": "email" },
                        "gender": { "type": "boolean", "enum": [true, false] },
                        "birthday": { "type": "string", "format": "date" },
                        "id_number": { "type": "string" },
                        "phone_number": { "type": "string" },
                        "role": {
                          "type": "object",
                          "properties": {
                            "role_id": { "type": "integer" },
                            "role_name": { "type": "string" }
                          }
                        },
                        "membership_type": {
                          "type": "object",
                          "properties": {
                            "membership_id": { "type": "integer" },
                            "membership_name": { "type": "string" },
                            "discount_rate": { "type": "number", "format": "float" }
                          }
                        }
                      }
                    },
                    {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "account_id": { "type": "integer" },
                          "full_name": { "type": "string" },
                          "email": { "type": "string", "format": "email" },
                          "gender": { "type": "boolean", "enum": [true, false] },
                          "birthday": { "type": "string", "format": "date" },
                          "id_number": { "type": "string" },
                          "phone_number": { "type": "string" },
                          "role": {
                            "type": "object",
                            "properties": {
                              "role_id": { "type": "integer" },
                              "role_name": { "type": "string" }
                            }
                          },
                          "membership_type": {
                            "type": "object",
                            "properties": {
                              "membership_id": { "type": "integer" },
                              "membership_name": { "type": "string" },
                              "discount_rate": { "type": "number", "format": "float" }
                            }
                          }
                        }
                      }
                    }
                  ]
                }
              }
            }
          }
        }
      },
      "put": {
        "tags": ["Account Controller"],
        "parameters": [
          {
            "in": "path",
            "name": "account_id",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "role_id": {
                    "type": "integer",
                    "description": "Role ID to update for the account"
                  },
                  "membership_id": {
                    "type": "integer",
                    "description": "Membership ID to update for the account"
                  },
                  "full_name": {
                    "type": "string",
                    "description": "Full name to update"
                  },
                  "gender": {
                    "type": "string",
                    "enum": [true, false],
                    "description": "Gender to update"
                  },
                  "birthday": {
                    "type": "string",
                    "format": "date",
                    "description": "Birthday to update (YYYY-MM-DD)"
                  },
                  "id_number": {
                    "type": "string",
                    "description": "ID number to update"
                  },
                  "phone_number": {
                    "type": "string",
                    "description": "Phone number to update"
                  },
                  "email": {
                    "type": "string",
                    "format": "email",
                    "description": "Email to update"
                  },
                  "password": {
                    "type": "string",
                    "description": "Password to update"
                  }
                },
                "example": {
                  "full_name": "Updated Name",
                  "email": "updated@example.com",
                  "phone_number": "987-654-3210"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "account_id": { "type": "integer" },
                    "full_name": { "type": "string" },
                    "email": { "type": "string", "format": "email" },
                    "gender": { "type": "boolean", "enum": [true, false] },
                    "birthday": { "type": "string", "format": "date" },
                    "id_number": { "type": "string" },
                    "phone_number": { "type": "string" },
                    "role": {
                      "type": "object",
                      "properties": {
                        "role_id": { "type": "integer" },
                        "role_name": { "type": "string" }
                      }
                    },
                    "membership_type": {
                      "type": "object",
                      "properties": {
                        "membership_id": { "type": "integer" },
                        "membership_name": { "type": "string" },
                        "discount_rate": { "type": "number", "format": "float" }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": ["Account Controller"],
        "parameters": [
          {
            "in": "path",
            "name": "account_id",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": { "type": "string", "example": "Account deleted successfully" }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "tags": []
}