          {
            "in": "path",
            "name": "movie_id",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Movie deleted"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
