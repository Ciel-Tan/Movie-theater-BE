          "membership_id": {
            "type": "integer",
            "nullable": true
          },
          "full_name": {
            "type": "string",
            "nullable": true
          },
          "gender": {
            "type": "boolean",
            "enum": [true, false],
            "nullable": true
          },
          "birthday": {
            "type": "string",
            "format": "date",
            "nullable": true
          },
          "id_number": {
            "type": "string",
            "nullable": true
          },
          "phone_number": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "format": "email",
            "nullable": true
          }
        }
      },
      "ChangePasswordRequest": {
        "type": "object",
        "properties": {
          "currentPassword": { "type": "string" },
          "newPassword": { "type": "string" }
        }
      },
      "Room": {
        "type": "object",
        "properties": {
          "room_id": {
            "type": "integer"
          },
          "room_name": {
            "type": "string"
          }
        }
      }
    }
