      "get": {
        "tags": ["Movie Controller"],
        "parameters": [
          {
            "in": "path",
            "name": "movie_id",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
         "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MovieDetail"
                }
              }
            }
          }
        }
      },
      "put": {
        "tags": ["Movie Controller"],
        "parameters": [
          {
            "in": "path",
            "name": "movie_id",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
