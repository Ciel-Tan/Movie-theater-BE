{
  "openapi": "3.0.0",
  "info": {
    "title": "Movie Theater API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:3000",
      "description": "Development server"
    }
  ],
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "description": "JWT Bearer token to access protected endpoints"
      }
    },
    "schemas": {
      "Genre": {
        "type": "object",
        "properties": {
          "genre_id": {
            "type": "integer"
          },
          "genre_name": {
            "type": "string"
          }
        }
      },
      "Director": {
        "type": "object",
        "properties": {
          "director_id": {
            "type": "integer"
          },
          "director_name": {
            "type": "string"
          }
        }
      },
      "Actor": {
        "type": "object",
        "properties": {
          "actor_id": {
            "type": "integer"
          },
          "actor_name": {
            "type": "string"
          }
        }
      },
      "Showtime": {
        "type": "object",
        "properties": {
          "showtime_id": {
            "type": "integer"
          },
          "movie_id": {
            "type": "integer"
          },
          "room_id": {
            "type": "integer"
          },
          "show_datetime": {
            "type": "string",
            "format": "date",
            "example": "2023-01-01T00:00:00.000Z"
          }
        }
      },
      "MovieDetail": {
        "type": "object",
        "properties": {
          "movie_id": {
            "type": "integer"
          },
          "title": {
            "type": "string"
          },
          "poster_image": {
            "type": "string",
            "nullable": true
          },
          "poster_url": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "age_rating": {
            "type": "integer",
            "nullable": true
          },
          "run_time": {
            "type": "integer",
            "nullable": true
          },
          "release_date": {
            "type": "string",
            "format": "date",
            "nullable": true
          },
          "trailer_link": {
            "type": "string",
            "nullable": true
          },
          "language": {
            "type": "string",
            "nullable": true
          },
          "genres": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Genre"
            }
          },
          "director": {
            "$ref": "#/components/schemas/Director"
          },
          "actors": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Actor"
            }
          },
          "showtime": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Showtime"
            }
          }
        }
      },
      "MovieUpdateRequest": {
        "type": "object",
        "properties": {
          "poster_image": {
            "type": "string",
            "nullable": true
          },
          "poster_url": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "age_rating": {
            "type": "integer",
            "nullable": true
          },
          "run_time": {
            "type": "integer",
            "nullable": true
          },
          "release_date": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "trailer_link": {
            "type": "string",
            "nullable": true
          },
          "language": {
            "type": "string",
            "nullable": true
          },
          "director_id": {
            "type": "integer",
            "nullable": true
          }
        }
      },
      "Login": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "LoginResponse": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string"
          },
          "expiresIn": {
            "type": "integer",
            "example": 3600
          }
        }
      },
      "Register": {
        "type": "object",
        "properties": {
          "full_name": {
            "type": "string"
          },
          "gender": {
            "type": "boolean",
            "enum": [true, false],
            "example": true
          },
          "birthday": {
            "type": "string",
            "format": "date",
            "example": "1990-01-15"
          },
          "id_number": {
            "type": "string"
          },
          "phone_number": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "RegisterResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "example": "Account created successfully"
          },
          "account": {
            "type": "object",
            "description": "Details of the newly created account"
          }
        }
      },
      "Role": {
        "type": "object",
        "properties": {
          "role_id": { "type": "integer" },
          "role_name": { "type": "string" }
        }
      },
      "Membership": {
        "type": "object",
        "properties": {
          "membership_id": { "type": "integer" },
          "membership_name": { "type": "string" },
          "discount_rate": { "type": "number", "format": "float" }
        }
      },
      "Account": {
        "type": "object",
        "properties": {
          "account_id": { "type": "integer" },
          "full_name": { "type": "string" },
          "email": { "type": "string", "format": "email" },
          "gender": { "type": "boolean", "enum": [true, false] },
          "birthday": { "type": "string", "format": "date" },
          "id_number": { "type": "string" },
          "phone_number": { "type": "string" },
          "role": {
            "$ref": "#/components/schemas/Role"
          },
          "membership_type": {
            "$ref": "#/components/schemas/Membership"
          }
        }
      },
      "AccountUpdateRequest": {
        "type": "object",
